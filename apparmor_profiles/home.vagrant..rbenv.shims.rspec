#include <tunables/global>

/home/vagrant/.rbenv/shims/rspec {
  #include <abstractions/base>

  /bin/bash ix,
  /bin/readlink rix,
  /dev/pts/0 rw,
  /dev/tty rw,
  /home/*/.rbenv/** mr,
  /home/vagrant/.rbenv/libexec/** rix,
  /home/vagrant/.rbenv/shims/ruby rix,
  /home/vagrant/.rbenv/versions/2.3.1/bin/** rix,
  /usr/bin/cut rix,
  "/usr/bin/env bash" ix,
  "/usr/bin/env ruby" ix,
  /usr/bin/head rix,
  /usr/bin/ruby1.9.1 rix,
  /vagrant/security_example/create_file_spec.rb r,

}